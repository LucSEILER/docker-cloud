{
  "appName": "Docker Cloud Application",
  "version": "1.0.0",
  "environment": "production",
  "features": {
    "authentication": true,
    "api": true,
    "documentation": true
  },
  "endpoints": {
    "frontend": "http://localhost:80",
    "backend": "http://localhost:3000",
    "docs": "http://localhost:8080"
  },
  "limits": {
    "maxRequests": 1000,
    "timeout": 30000
  }
}
